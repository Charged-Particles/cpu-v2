{
  "name": "cpu-v2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Charged-Particles/cpu-v2.git",
  "author": "Firma Lux, Inc. <charged.fi>",
  "license": "MIT",
  "scripts": {
    "reinstall": "yarn clean && rm -rf node_modules && rm -f yarn.lock && yarn",
    "clean": "yarn clean-build && yarn clean-test && hardhat clean",
    "clean-test": "rm -rf test-results.xml coverage coverage.json test-results.xml",
    "clean-build": "rm -rf abis build cache-zk artifacts-zk typechain-types",
    "compile": "hardhat compile --show-stack-traces --max-memory 8192",
    "test": "export IS_TEST=true && yarn clean-test && hardhat test --show-stack-traces --network hardhat",
    "watch-test": "hardhat watch test",
    "coverage": "yarn clean-test && hardhat --show-stack-traces coverage --temp coverage_build && yarn clean-test",
    "gas": "REPORT_GAS=true yarn test",
    "deploy-cpu-only": "hardhat deploy-zksync --script Setup_ChargedParticles.ts --network",
    "deploy-bufficorn": "hardhat deploy-zksync --script Setup_Bufficorn.ts --network",
    "deploy-test-script": "hardhat deploy-zksync --script ZkSepoliaTest.ts --network"
  },
  "devDependencies": {
    "@matterlabs/hardhat-zksync-chai-matchers": "^1.3.0",
    "@matterlabs/hardhat-zksync-deploy": "^1.3.0",
    "@matterlabs/hardhat-zksync-ethers": "^1.0.0",
    "@matterlabs/hardhat-zksync-node": "^1.0.3",
    "@matterlabs/hardhat-zksync-solc": "^1.1.4",
    "@matterlabs/hardhat-zksync-verify": "^1.4.2",
    "@matterlabs/zksync-contracts": "^0.6.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-verify": "^2.0.5",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "4.6.0",
    "@openzeppelin/contracts-upgradeable": "4.6.0",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.11.16",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "erc20permit": "^0.1.1",
    "ethers": "^6.10.0",
    "hardhat": "^2.12.4",
    "hardhat-abi-exporter": "^2.10.1",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^4.9.5",
    "zksync-ethers": "^6.2.0"
  }
}
